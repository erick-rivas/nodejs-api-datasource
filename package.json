{
  "name": "nodejs-datasource",
  "version": "1.0.0",
  "description": "Repository of Node.js API datasource",
  "private": true,
  "apidoc": {
    "title": "Node.js Datasource",
    "name": "Node.js Datasource",
    "sampleUrl": "http://localhost:4004",
    "order": [
      "get",
      "post",
      "put",
      "delete"
    ]
  },
  "scripts": {
    "start": "tsc && node ./bin/www",
    "debug": "tsc && node dist/debug.js"
  },
  "dependencies": {
    "@types/node": "12.0.3",
    "@types/request": "2.48.1",
    "archiver": "3.0.0",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "csv-parse": "4.4.3",
    "debug": "4.1.1",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-slow-down": "1.3.1",
    "faker": "4.1.0",
    "helmet": "3.18.0",
    "html-pdf": "2.2.0",
    "i": "0.3.6",
    "log": "6.0.0",
    "module-alias": "2.2.0",
    "morgan": "1.9.1",
    "multer": "1.4.1",
    "mysql": "2.17.1",
    "nodemailer": "6.2.1",
    "qrcode": "^1.3.3",
    "request": "2.88.0",
    "serve-favicon": "2.5.0",
    "typescript": "3.5.1"
  },
  "_moduleAliases": {
    "@lt": "dist/src",
    "@support": "dist/src/support",
    "@util": "dist/src/util"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "routes",
      "docs",
      "app.ts"
    ],
    "ext": "ts"
  }
}
